# Kill-FOMO Multi-Agent System Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# XMTP Configuration
XMTP_ENV=production
WALLET_PRIVATE_KEY=your_wallet_private_key_here
ENCRYPTION_KEY=your_32_byte_hex_encryption_key_here

# CDP v2 Server Wallet & Data APIs
CDP_API_KEY_ID=your_cdp_api_key_id
CDP_API_KEY_SECRET=your_cdp_api_key_secret
CDP_WALLET_SECRET=your_cdp_wallet_secret

# Logging Configuration
LOG_LEVEL=info
AGENT_LOG_LEVEL=info

# Network Configuration
NETWORK_ID=base-mainnet
RPC_URL=https://mainnet.base.org

# x402 Revenue Generation (Code NYC Bounty Track)
X402_SELLER_ADDRESS=0xYourUSDCReceivingAddress
X402_PRICE_USDC_CENTS=100
X402_FACILITATOR_URL=https://facilitator.x402.org

# Smart Wallet Configuration (Code NYC Bounty Track)
ENABLE_GAS_SPONSORSHIP=true
ENABLE_BATCH_TRANSACTIONS=true
SMART_WALLET_TYPE=eip4337

# MCP Server Configuration (Code NYC Bounty Track)
MCP_SERVER_NAME=based-agents-mcp-server
MCP_SERVER_VERSION=1.0.0
MCP_TRANSPORT=stdio

# Database Configuration (Optional - for persistent storage)
DATABASE_URL=postgresql://user:password@localhost:5432/baseagents

# Redis Configuration (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Application Configuration
NODE_ENV=production
PORT=3000

# Feature Flags
ENABLE_BLOCKCHAIN_INTEGRATION=true
ENABLE_PRICE_FEEDS=true
ENABLE_CONTENT_CURATION=true
ENABLE_GAME_BETTING=false

# Rate Limiting
MAX_MESSAGES_PER_MINUTE=10
MAX_CONVERSATIONS_PER_USER=5

# Security Configuration
JWT_SECRET=your_jwt_secret_here
CORS_ORIGINS=http://localhost:3000,https://your-domain.com

# External API Keys (Optional)
COINGECKO_API_KEY=your_coingecko_api_key
ETHERSCAN_API_KEY=your_etherscan_api_key
BASESCAN_API_KEY=your_basescan_api_key

# Webhook Configuration (Optional)
WEBHOOK_SECRET=your_webhook_secret
WEBHOOK_URL=https://your-webhook-endpoint.com/webhooks

# Analytics Configuration (Optional)
MIXPANEL_TOKEN=your_mixpanel_token
GOOGLE_ANALYTICS_ID=your_google_analytics_id 