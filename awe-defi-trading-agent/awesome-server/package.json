{
  "name": "mcp-langchain-backend",
  "version": "1.0.0",
  "description": "Backend service for LangChain and MCP integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate:up": "node dist/scripts/migrate-database.js up",
    "migrate:down": "node dist/scripts/migrate-database.js down",
    "migrate:up:dev": "node --loader ts-node/esm src/scripts/migrate-database.ts up",
    "migrate:down:dev": "node --loader ts-node/esm src/scripts/migrate-database.ts down",
    "db:setup": "npm run migrate:up",
    "test:db": "NODE_ENV=test DB_SSL_MODE=disable node --loader ts-node/esm test/test-database.js",
    "test:db:ssl": "NODE_ENV=test DB_SSL_REJECT_UNAUTHORIZED=false node --loader ts-node/esm test/test-database.js",
    "test:payment": "node test/test-payment-flow.js",
    "test:payment:complete": "node test/test-payment-flow-complete.js",
    "test:payment:quick": "sh test/quick-payment-test.sh",
    "test:s3-avatar": "npm run build && node test/test-s3-avatar.js",
    "download-mcp-logos": "node scripts/download-mcp-logos.js",
    "cleanup:tokens": "tsx src/scripts/cleanup-duplicate-tokens.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@langchain/community": "^0.0.40",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/mcp-adapters": "^0.5.2",
    "@langchain/openai": "^0.5.11",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "coinbase-commerce-node": "^1.0.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.14.3",
    "eventsource": "^4.0.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.0",
    "node-cache": "^5.1.2",
    "pg": "^8.16.1",
    "pg-hstore": "^2.3.4",
    "portfinder": "^1.0.37",
    "sequelize": "^6.37.7",
    "siwe": "^2.3.2",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^9.0.8",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  }
}
