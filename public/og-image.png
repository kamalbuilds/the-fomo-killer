<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630" width="1200" height="630">
  <!-- Background -->
  <defs>
    <linearGradient id="ogGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0F172A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1E293B;stop-opacity:1" />
    </linearGradient>
    
    <pattern id="dots" width="30" height="30" patternUnits="userSpaceOnUse">
      <circle cx="15" cy="15" r="1" fill="#334155" opacity="0.5"/>
    </pattern>
  </defs>
  
  <rect width="1200" height="630" fill="url(#ogGradient)"/>
  <rect width="1200" height="630" fill="url(#dots)"/>
  
  <!-- Left side - Logo and Title -->
  <g transform="translate(350, 315)">
    <!-- Large logo -->
    <g transform="scale(2.5)">
      <!-- Head -->
      <rect x="-40" y="-50" width="80" height="60" rx="10" fill="white" opacity="0.95"/>
      
      <!-- Eyes -->
      <circle cx="-15" cy="-25" r="8" fill="#3B82F6"/>
      <circle cx="15" cy="-25" r="8" fill="#3B82F6"/>
      <circle cx="-15" cy="-25" r="4" fill="white"/>
      <circle cx="15" cy="-25" r="4" fill="white"/>
      
      <!-- Body chart -->
      <rect x="-25" y="15" width="8" height="25" fill="#10B981" opacity="0.9"/>
      <rect x="-12" y="5" width="8" height="35" fill="#3B82F6" opacity="0.9"/>
      <rect x="1" y="20" width="8" height="20" fill="#8B5CF6" opacity="0.9"/>
      <rect x="14" y="10" width="8" height="30" fill="#F59E0B" opacity="0.9"/>
    </g>
    
    <!-- Title -->
    <text y="120" text-anchor="middle" font-family="Arial, sans-serif" font-size="56" font-weight="bold" fill="white">
      KILL-FOMO
    </text>
    
    <!-- Subtitle -->
    <text y="160" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" fill="#94A3B8">
      AI-Powered DeFi Intelligence
    </text>
  </g>
  
  <!-- Right side - Features -->
  <g transform="translate(850, 200)">
    <!-- Feature list -->
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ Multi-Agent System</text>
    <text x="0" y="35" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ CDP AgentKit v2</text>
    <text x="0" y="70" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ x402 Micropayments</text>
    <text x="0" y="105" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ Real-time Analytics</text>
    <text x="0" y="140" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ Smart Wallets</text>
    <text x="0" y="175" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ XMTP Messaging</text>
    <text x="0" y="210" font-family="Arial, sans-serif" font-size="18" fill="#10B981">✓ Farcaster Native</text>
  </g>
  
  <!-- Bottom badges -->
  <g transform="translate(600, 550)">
    <g transform="translate(-250, 0)">
      <rect x="-40" y="-12" width="80" height="24" rx="12" fill="#3B82F6" opacity="0.2"/>
      <text text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#3B82F6">BASE</text>
    </g>
    
    <g transform="translate(-125, 0)">
      <rect x="-40" y="-12" width="80" height="24" rx="12" fill="#8B5CF6" opacity="0.2"/>
      <text text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#8B5CF6">CDP</text>
    </g>
    
    <g transform="translate(0, 0)">
      <rect x="-40" y="-12" width="80" height="24" rx="12" fill="#10B981" opacity="0.2"/>
      <text text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#10B981">MiniKit</text>
    </g>
    
    <g transform="translate(125, 0)">
      <rect x="-40" y="-12" width="80" height="24" rx="12" fill="#F59E0B" opacity="0.2"/>
      <text text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#F59E0B">x402</text>
    </g>
    
    <g transform="translate(250, 0)">
      <rect x="-40" y="-12" width="80" height="24" rx="12" fill="#EC4899" opacity="0.2"/>
      <text text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#EC4899">XMTP</text>
    </g>
  </g>
</svg>