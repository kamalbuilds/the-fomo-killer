<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1280 720" width="1280" height="720">
  <!-- Screenshot 1: Chat Interface -->
  
  <!-- Browser frame -->
  <rect width="1280" height="720" fill="#0F172A"/>
  
  <!-- App interface -->
  <rect x="20" y="60" width="1240" height="640" rx="8" fill="#1E293B"/>
  
  <!-- Header -->
  <rect x="20" y="60" width="1240" height="70" rx="8" fill="#334155"/>
  <text x="60" y="105" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="white">KILL-FOMO</text>
  <text x="200" y="105" font-family="Arial, sans-serif" font-size="14" fill="#94A3B8">AI DeFi Agents</text>
  
  <!-- Status indicator -->
  <circle cx="1200" cy="95" r="8" fill="#10B981"/>
  <text x="1150" y="100" font-family="Arial, sans-serif" font-size="12" fill="#10B981" text-anchor="end">Online</text>
  
  <!-- Chat area -->
  <rect x="40" y="150" width="1200" height="400" rx="4" fill="#0F172A"/>
  
  <!-- User message -->
  <g transform="translate(1000, 200)">
    <rect x="-180" y="-25" width="200" height="50" rx="20" fill="#3B82F6"/>
    <text x="-80" y="5" font-family="Arial, sans-serif" font-size="14" fill="white" text-anchor="middle">
      Show trending tokens
    </text>
  </g>
  
  <!-- Agent response -->
  <g transform="translate(80, 280)">
    <rect x="0" y="0" width="600" height="200" rx="20" fill="#334155"/>
    <text x="20" y="30" font-family="Arial, sans-serif" font-size="14" fill="white">
      Top 5 Trending Tokens on Base:
    </text>
    
    <!-- Token list -->
    <rect x="20" y="50" width="560" height="40" rx="4" fill="#1E293B"/>
    <text x="30" y="75" font-family="Arial, sans-serif" font-size="12" fill="#10B981">1. ETH</text>
    <text x="150" y="75" font-family="Arial, sans-serif" font-size="12" fill="#94A3B8">$3,521.45</text>
    <text x="300" y="75" font-family="Arial, sans-serif" font-size="12" fill="#10B981">+5.2%</text>
    
    <rect x="20" y="95" width="560" height="40" rx="4" fill="#1E293B"/>
    <text x="30" y="120" font-family="Arial, sans-serif" font-size="12" fill="#10B981">2. USDC</text>
    <text x="150" y="120" font-family="Arial, sans-serif" font-size="12" fill="#94A3B8">$1.00</text>
    <text x="300" y="120" font-family="Arial, sans-serif" font-size="12" fill="#94A3B8">0.0%</text>
    
    <rect x="20" y="140" width="560" height="40" rx="4" fill="#1E293B"/>
    <text x="30" y="165" font-family="Arial, sans-serif" font-size="12" fill="#10B981">3. BRETT</text>
    <text x="150" y="165" font-family="Arial, sans-serif" font-size="12" fill="#94A3B8">$0.127</text>
    <text x="300" y="165" font-family="Arial, sans-serif" font-size="12" fill="#10B981">+12.7%</text>
  </g>
  
  <!-- Input area -->
  <rect x="40" y="570" width="1100" height="50" rx="25" fill="#334155"/>
  <text x="60" y="600" font-family="Arial, sans-serif" font-size="14" fill="#64748B">Ask about DeFi, swaps, portfolio...</text>
  
  <!-- Send button -->
  <rect x="1160" y="570" width="80" height="50" rx="25" fill="#3B82F6"/>
  <text x="1200" y="600" font-family="Arial, sans-serif" font-size="14" fill="white" text-anchor="middle">Send</text>
  
  <!-- Agent indicators -->
  <g transform="translate(40, 640)">
    <rect x="0" y="0" width="100" height="30" rx="15" fill="#3B82F6" opacity="0.2"/>
    <text x="50" y="20" font-family="Arial, sans-serif" font-size="11" fill="#3B82F6" text-anchor="middle">SwapAgent</text>
    
    <rect x="110" y="0" width="100" height="30" rx="15" fill="#10B981" opacity="0.2"/>
    <text x="160" y="20" font-family="Arial, sans-serif" font-size="11" fill="#10B981" text-anchor="middle">TokenTracker</text>
    
    <rect x="220" y="0" width="100" height="30" rx="15" fill="#8B5CF6" opacity="0.2"/>
    <text x="270" y="20" font-family="Arial, sans-serif" font-size="11" fill="#8B5CF6" text-anchor="middle">Portfolio</text>
  </g>
</svg>